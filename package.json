{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/bin/www",
    "devstart": "set DEBUG=dev && babel-watch ./src/bin/www",
    "build": "rm -rf dist && mkdir dist && babel src -s -d dist",
    "test": "set NODE_ENV=test&& npm run reset:db && mocha -r @babel/register --recursive src/tests/ --exit --timeout 10000",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate": "codeclimate-test-reporter > lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls && npm run codeclimate",
    "generate-report": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "migrate": "sequelize db:migrate",
    "seeder": "sequelize db:seed:all",
    "drop:db": "sequelize db:migrate:undo:all",
    "reset:db": "sequelize db:migrate:undo:all && npm run migrate && npm run seeder"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.6",
    "googleapis": "^42.0.0",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mocha": "^6.2.0",
    "morgan": "^1.9.1",
    "node-linkedin": "^0.5.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^5.18.1",
    "sequelize-cli": "^5.5.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-watch": "^7.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.6",
    "debug": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  }
}
